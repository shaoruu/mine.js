    "file-loader": "^6.2.0",
    "gulp": "^4.0.2",
    "happypack": "^5.0.1",
    "mini-css-extract-plugin": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "protobufjs": "^6.11.3",
    "svelte": "^3.49.0",
    "svelte-check": "^1.0.46",
    "svelte-loader": "^3.0.0",
    "svelte-preprocess": "^4.3.0",
    "thread-loader": "^3.0.3",
    "ts-loader": "^8.0.4",
    "typescript": "^4.2.4",
    "url-loader": "^4.1.1",
    "uuid": "^8.3.2",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
  },
  "scripts": {
    "client": "webpack",
    "watch": "gulp dev -L",
    "server": "cross-env RUST_BACKTRACE=1 cargo watch -c -w server -w assets/metadata -x \"run --release\"",
    "server:kill": "kill -9 $(lsof -ti:4000)",
    "server:mapids": "cross-env RUST_BACKTRACE=1 cargo run -p server_tasks --bin map_ids --release",
    "server:clean": "cross-env RUST_BACKTRACE=1 cargo run -p server_tasks --bin clean_chunks --release",
    "server:script": "cross-env RUST_BACKTRACE=1 cargo run -p server_tasks --bin script --release",
    "server:udeps": "cargo +nightly udeps",
    "build": "cross-env NODE_ENV=production webpack",
    "compile:ts": "pbts -o protocol/index.d.ts protocol/index.js",
    "compile:js": "pbjs -t static-module -w commonjs -o protocol/index.js protocol/messages.proto",
    "compile": "yarn run compile:js && yarn run compile:ts",
    "electron": "electron-forge start",
    "test": "cross-env RUST_BACKTRACE=1 cargo test --all -- --nocapture",
    "dev": "npm-run-all --parallel server client watch",
    "validate": "svelte-check"
  },
  "dependencies": {
    "@mapbox/node-pre-gyp": "^1.0.3",
    "@tweenjs/tween.js": "^18",
    "@types/query-string": "^6.3.0",
    "@types/three": "^0.127.1",
    "buffer": "^6.0.3",
    "deepmerge": "^4.2.2",
    "domurl": "^2.3.4",
    "fast-voxel-raycast": "^0.1.1",
    "gl-vec3": "^1.1.3",
    "is-electron": "^2.2.0",
    "mousetrap": "^1.6.5",
    "ndarray": "^1.0.19",
    "noisejs": "^2.1.0",
    "pako": "^2.0.3",
    "query-string": "^7.0.0",
    "random-name": "^0.1.2",
    "raw-loader": "^4.0.2",
    "source-map-support": "^0.5.19",
    "static-kdtree": "^1.0.2",
    "stats.js": "^0.17.0",
    "svelte-routing": "^1.6.0",
    "svelte-spa-router": "^3.2.0",
    "three": "^0.137.0",
    "three-spritetext": "^1.6.2",
    "tweakpane": "^3.0.3",
    "typedarray-pool": "^1.2.0",
    "voxel-aabb-sweep": "^0.5.0"
  }
}
